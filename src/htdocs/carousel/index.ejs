<script src="/assets/libs/slick/slick.js" defer></script>
<!-- <script src="/assets/js/common.js" defer></script> -->
<script>
$(function(){
	
	$('.carousel_2').slick({
		dots: true,
		centerMode: true,
		infinite: true,
		centerPadding: 0,
		slidesToShow: 1,
	});
	
});
</script>
</body>
</html>